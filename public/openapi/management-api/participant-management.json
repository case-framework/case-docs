    "/v1/studies/{studyKey}/participants/{participantID}/responses": {
        "get": {
            "tags": [
                "participants"
            ],
            "operationId": "participants_listParticipantResponses",
            "summary": "List participant responses",
            "security": [
                {
                    "BearerAuth": []
                }
            ],
            "parameters": [
                {
                    "name": "studyKey",
                    "in": "path",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "participantID",
                    "in": "path",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "page",
                    "in": "query",
                    "schema": {
                        "type": "integer",
                        "minimum": 1
                    }
                },
                {
                    "name": "limit",
                    "in": "query",
                    "schema": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 100
                    }
                },
                {
                    "name": "filter",
                    "in": "query",
                    "description": "bson.M object to filter responses",
                    "schema": {
                        "type": "object",
                        "additionalProperties": {
                            "type": "object"
                        }
                    }
                },
                {
                    "name": "sort",
                    "in": "query",
                    "description": "bson.M object to sort responses",
                    "schema": {
                        "type": "object",
                        "additionalProperties": {
                            "type": "object"
                        }
                    }
                }
            ],
            "responses": {
                "200": {
                    "description": "List participant responses",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object", 
                                "properties": {
                                    "responses": {"type": "array", "items": {"$ref": "../management-api.json#/components/schemas/SurveyResponse"}}, 
                                    "pagination": {"$ref": "../management-api.json#/components/schemas/PaginationInfos" } 
                                },
                                "required": [
                                    "responses",
                                    "pagination"
                                ]
                            }
                        }
                    }
                },
                "400": {
                    "$ref": "../management-api.json#/components/responses/Error400"
                },
                "401": {
                    "$ref": "../management-api.json#/components/responses/Error401"
                },
                "500": {
                    "$ref": "../management-api.json#/components/responses/Error500"
                }
            }
        },
        "post": {
            "tags": [
                "participants"
            ],
            "operationId": "participants_submitParticipantResponse",
            "summary": "Submit participant response",
            "security": [
                {
                    "BearerAuth": []
                }
            ],
            "parameters": [
                {
                    "name": "studyKey",
                    "in": "path",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "name": "participantID",
                    "in": "path",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
            ],
            "requestBody": {
                "required": true,
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "../management-api.json#/components/schemas/SurveyResponse"
                        }
                    }
                }
            },
            "responses": {
                "200": {
                    "description": "Submit participant response",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "message": { "type": "string" },
                                    "result": {
                                        "type": "array",
                                        "items": {
                                            "$ref": "../management-api.json#/components/schemas/AssignedSurvey"
                                        }
                                    },
                                    "participant": {
                                        "$ref": "../management-api.json#/components/schemas/Participant"
                                    }
                                },
                                "required": [
                                    "message", "result", "participant"
                                ]
                            }
                        }
                    }
                },
                "400": {
                    "$ref": "../management-api.json#/components/responses/Error400"
                },
                "401": {
                    "$ref": "../management-api.json#/components/responses/Error401"
                },
                "500": {
                    "$ref": "../management-api.json#/components/responses/Error500"
                }
            }
        }
    },
